class dict_test {
    @main
    static def main() {
        person = {
            name : "Jinwoo",
            age  : 21,
            hobby: ["Game", "AVR", "Buying Thinkpad X1"]
        };

        for (k,v in person) {
            puts (k);
            puts (v);
            
            if (v is array) {
                print_ary(v);
            }
        } 
    }
    
    static def print_ary(ary) {
        for (k in ary) {
            puts (k);
        }
    }
}
