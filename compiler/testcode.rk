class example_object {
    @main
    static def main() {
        f = foo.new();
        f.v = 1234;
        f.say_hello();

        f = foo.new();
        f.v = 5678;
        f.say_hello();
    }
}

class foo {
    def say_hello() {
        hello = "hello";

        for (i=0;i<hello.length();i++)
            print(hello[i]);

        print(this.v);
    }
}
